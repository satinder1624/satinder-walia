<div class="banner-buttons">
  <a href="{{section.settings.button_link_1}}" class="animated-button">
    {{section.settings.button_text_1}}
  </a>
  <a href="{{section.settings.button_link_2}}" class="animated-button">
    {{section.settings.button_text_2}}
  </a>
</div>

{% schema %}
{
  "name": "Banner with Buttons",
  "settings": [
    {
      "type": "text",
      "id": "button_text_1",
      "label": "Button Text 1",
      "default": "Hello"
    },
    {
      "type": "text",
      "id": "button_link_1",
      "label": "Button Link 1",
      "default": "https://www.youtube.com/"
    },
    {
      "type": "text",
      "id": "button_text_2",
      "label": "Button Text 2",
      "default": "Hello 2"
    },
    {
      "type": "text",
      "id": "button_link_2",
      "label": "Button Link 2",
      "default": "https://www.google.com/"
    }
  ],
  "presets": [
    {
      "name": "Banner with Buttons",
      "category": "Custom Sections"
    }
  ]
}
{% endschema %}
